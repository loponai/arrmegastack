# ==========================================
# MEGASTACK - Environment Template
# Copy to /opt/megastack/.env and fill in values
# Or run: megastack install
# ==========================================

# System
TZ=America/Los_Angeles
MS_ROOT=/opt/megastack
MS_DOMAIN=localhost
MS_ADMIN_PASSWORD_HASH=
MS_SESSION_SECRET=
MS_BACKUP_KEY=

# VPN (Media)
VPN_PROVIDER=nordvpn
VPN_TYPE=openvpn
VPN_USER=
VPN_PASSWORD=
SERVER_COUNTRIES=United States
MEDIA_ROOT=/opt/megastack/modules/media/media

# Optional
NOTIFIARR_API_KEY=
